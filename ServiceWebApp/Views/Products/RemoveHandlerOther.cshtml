@model ServiceWebApp.Models.Model
@{
    ViewBag.Title = "RemoveHandlerOther";
}
@using ServiceWebApp.Controllers;
<header title="Are you sure you want to remove this item?" class="title">Are you sure you want to remove this item?</header>
<body class="tabcontent">
    @{
        var x = (string)ViewData["path"];
    }
    <div class="modal-footer">
        <a href="#">@Html.ActionLink("Cancel", "GoBack", null,  new { onclick = "disable()", id = "mylink", @class = "btn btn-default" })</a>
        <a href="#">@Html.ActionLink("Yes", "RemoveHandler", new { path = x }, new { onclick = "disable()", id = "mylink", @class = "btn btn-default" })</a>


    </div>



</body>

<style>

    .modal-content {
        color: #001198;
        background-color: #88c8ed;
        font-family: 'Courier New'
    }

    .ok {
        background-color: white;
        color: #001198;
        font-family: 'Courier New';
        font-size: 10pt;
    }

    .btn {
        color: #001198;
        font-family: 'Courier New'
    }

    .modal-title {
        color: #001198;
        font-family: 'Courier New'
    }


    .title {
        color: #a5dded;
        font-size: 40px;
        background-color: black;
        font-family: 'Courier New';
        font-weight: bold;
        padding: 30px 0px 0px 0px;
    }

    .tabcontent {
        color: white;
        background-color: black;
        font-family: 'Courier New';
        font-size: 25pt;
    }
</style>

